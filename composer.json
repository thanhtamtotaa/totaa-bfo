{
    "name": "totaa/totaa-bfo",
    "description": "Link User with BFO info",
    "keywords": [
        "totaa",
        "totaa-bfo"
    ],
    "homepage": "https://github.com/thanhtamtotaa/totaa-bfo",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tho Thanh TÃ¢m",
            "email": "thanhtamtotaa@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/fortify": "^1.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "wildside/userstamps": "^2.1",
        "totaa/totaa-permission": "^0.0.5"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Totaa\\TotaaBfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Totaa\\TotaaBfo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Totaa\\TotaaBfo\\TotaaBfoServiceProvider"
            ],
            "aliases": {
                "TotaaBfo": "Totaa\\TotaaBfo\\TotaaBfoFacade"
            }
        }
    }
}
